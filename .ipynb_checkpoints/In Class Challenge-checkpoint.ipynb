    "import numpy as np\n",
    "def piSeries(n):\n",
    "    value = 1\n",
    "    for i in range(1,n):\n",
    "        value = value + (((-1)**i)/(2*i+1))\n",
    "        print(value)\n",
    "    return value*4\n",
    "for i in range(1,5000):\n",
    "    if i % 100 == 0:\n",
    "        print((np.pi - piSeries(i)/np.pi))"
   ]
