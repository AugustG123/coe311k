    "def binary(base10):\n",
    "    r = base10 % 2\n",
    "    q = int(base10/2)\n",
    "    ReversedRet = str(r)\n",
    "    while (q != 0):\n",
    "        r = q % 2\n",
    "        q = int(q / 2)\n",
    "        ReversedRet += str(r)\n",
    "    \n",
    "    return ReversedRet[::-1]\n",
    "binary(2025)"
   ]
